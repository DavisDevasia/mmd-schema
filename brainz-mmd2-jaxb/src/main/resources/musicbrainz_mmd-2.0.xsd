<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified"
           targetNamespace="http://musicbrainz.org/ns/mmd-2.0#"
           xmlns:ext="http://musicbrainz.org/ns/ext#-2.0"
           xmlns:mmd-2.0="http://musicbrainz.org/ns/mmd-2.0#">
  <xs:import schemaLocation="local.xsd"/>
  <xs:import namespace="http://musicbrainz.org/ns/ext#-2.0" schemaLocation="extensions.xsd"/>
  <xs:element name="metadata">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="mmd-2.0:artist"/>
        <xs:element minOccurs="0" ref="mmd-2.0:release"/>
        <xs:element minOccurs="0" ref="mmd-2.0:release-group"/>
        <xs:element minOccurs="0" ref="mmd-2.0:recording"/>
        <xs:element minOccurs="0" ref="mmd-2.0:label"/>
        <xs:element minOccurs="0" ref="mmd-2.0:work"/>
        <xs:element minOccurs="0" ref="mmd-2.0:puid"/>
        <xs:element minOccurs="0" ref="mmd-2.0:disc"/>
        <xs:element minOccurs="0" ref="mmd-2.0:artist-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:release-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:release-group-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:recording-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:label-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:work-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:isrc-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:annotation-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:cdstub-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:freedb-disc-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:tag-list"/>
        <xs:group ref="mmd-2.0:def_metadata-element_extension"/>
      </xs:sequence>
      <xs:attribute name="generator" type="xs:anyURI"/>
      <xs:attribute name="created" type="xs:dateTime"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="artist">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="mmd-2.0:name"/>
        <xs:element minOccurs="0" ref="mmd-2.0:sort-name"/>
        <xs:element minOccurs="0" ref="mmd-2.0:gender"/>
        <xs:element minOccurs="0" name="country" type="xs:string"/>
        <xs:element minOccurs="0" ref="mmd-2.0:disambiguation"/>
        <xs:element minOccurs="0" ref="mmd-2.0:life-span"/>
        <xs:element minOccurs="0" ref="mmd-2.0:alias-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:release-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:release-group-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:label-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:work-list"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="mmd-2.0:relation-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:tag-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:user-tag-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:rating"/>
        <xs:element minOccurs="0" ref="mmd-2.0:user-rating"/>
        <xs:group ref="mmd-2.0:def_artist-element_extension"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:anyURI"/>
      <xs:attribute name="type" type="xs:anyURI"/>
      <xs:attributeGroup ref="mmd-2.0:def_artist-attribute_extension"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="name" type="xs:string"/>
  <xs:element name="sort-name" type="xs:string"/>
  <xs:element name="gender" type="xs:string"/>
  <xs:element name="disambiguation" type="xs:string"/>
  <xs:element name="life-span">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="mmd-2.0:begin"/>
        <xs:element minOccurs="0" ref="mmd-2.0:end"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="begin" type="mmd-2.0:def_incomplete-date"/>
  <xs:element name="end" type="mmd-2.0:def_incomplete-date"/>
  <xs:element name="release">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="mmd-2.0:title"/>
        <xs:element minOccurs="0" ref="mmd-2.0:status"/>
        <xs:element minOccurs="0" ref="mmd-2.0:disambiguation"/>
        <xs:element minOccurs="0" ref="mmd-2.0:packaging"/>
        <xs:element minOccurs="0" ref="mmd-2.0:text-representation"/>
        <xs:element minOccurs="0" ref="mmd-2.0:artist-credit"/>
        <xs:element minOccurs="0" ref="mmd-2.0:release-group"/>
        <xs:element ref="mmd-2.0:date"/>
        <xs:element minOccurs="0" name="country" type="mmd-2.0:def_iso-3166"/>
        <xs:element minOccurs="0" ref="mmd-2.0:barcode"/>
        <xs:element minOccurs="0" ref="mmd-2.0:asin"/>
        <xs:element minOccurs="0" ref="mmd-2.0:label-info-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:medium-list"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="mmd-2.0:relation-list"/>
        <xs:group ref="mmd-2.0:def_release-element_extension"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:anyURI"/>
      <xs:attributeGroup ref="mmd-2.0:def_release-attribute_extension"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="title" type="xs:string"/>
  <xs:element name="status" type="xs:string"/>
  <xs:element name="packaging" type="xs:string"/>
  <xs:element name="text-representation">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="mmd-2.0:language"/>
        <xs:element minOccurs="0" ref="mmd-2.0:script"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="language" type="mmd-2.0:def_iso-639"/>
  <xs:element name="script" type="mmd-2.0:def_iso-15924"/>
  <xs:element name="date" type="mmd-2.0:def_incomplete-date"/>
  <xs:element name="barcode" type="xs:string"/>
  <xs:element name="asin">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:pattern value="[A-Z0-9]{10}"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="release-group">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="mmd-2.0:title"/>
        <xs:element minOccurs="0" ref="mmd-2.0:comment"/>
        <xs:element minOccurs="0" ref="mmd-2.0:artist-credit"/>
        <xs:element minOccurs="0" ref="mmd-2.0:release-list"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="mmd-2.0:relation-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:tag-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:user-tag-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:rating"/>
        <xs:element minOccurs="0" ref="mmd-2.0:user-rating"/>
        <xs:group ref="mmd-2.0:def_release-group-element_extension"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:anyURI"/>
      <xs:attribute name="type" type="xs:anyURI"/>
      <xs:attributeGroup ref="mmd-2.0:def_release-group-attribute_extension"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="comment" type="xs:string"/>
  <xs:element name="recording">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="mmd-2.0:title"/>
        <xs:element minOccurs="0" ref="mmd-2.0:length"/>
        <xs:element minOccurs="0" ref="mmd-2.0:disambiguation"/>
        <xs:element minOccurs="0" ref="mmd-2.0:artist-credit"/>
        <xs:element minOccurs="0" ref="mmd-2.0:release-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:puid-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:isrc-list"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="mmd-2.0:relation-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:tag-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:user-tag-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:rating"/>
        <xs:element minOccurs="0" ref="mmd-2.0:user-rating"/>
        <xs:group ref="mmd-2.0:def_recording-element_extension"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:anyURI"/>
      <xs:attributeGroup ref="mmd-2.0:def_recording-attribute_extension"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="length" type="xs:nonNegativeInteger"/>
  <xs:element name="label">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="mmd-2.0:name"/>
        <xs:element minOccurs="0" ref="mmd-2.0:sort-name"/>
        <xs:element minOccurs="0" ref="mmd-2.0:label-code"/>
        <xs:element minOccurs="0" ref="mmd-2.0:disambiguation"/>
        <xs:element minOccurs="0" name="country" type="mmd-2.0:def_iso-3166"/>
        <xs:element minOccurs="0" ref="mmd-2.0:life-span"/>
        <xs:element minOccurs="0" ref="mmd-2.0:alias-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:release-list"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="mmd-2.0:relation-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:tag-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:user-tag-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:rating"/>
        <xs:element minOccurs="0" ref="mmd-2.0:user-rating"/>
        <xs:group ref="mmd-2.0:def_label-element_extension"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:anyURI"/>
      <xs:attribute name="type" type="xs:anyURI"/>
      <xs:attributeGroup ref="mmd-2.0:def_label-attribute_extension"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="label-code" type="xs:nonNegativeInteger"/>
  <xs:element name="work">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="mmd-2.0:title"/>
        <xs:element minOccurs="0" ref="mmd-2.0:artist-credit"/>
        <xs:element minOccurs="0" ref="mmd-2.0:iswc"/>
        <xs:element minOccurs="0" ref="mmd-2.0:disambiguation"/>
        <xs:element minOccurs="0" ref="mmd-2.0:tag-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:user-tag-list"/>
        <xs:element minOccurs="0" ref="mmd-2.0:rating"/>
        <xs:element minOccurs="0" ref="mmd-2.0:user-rating"/>
        <xs:group ref="mmd-2.0:def_work-element_extension"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:anyURI"/>
      <xs:attribute name="type" type="xs:anyURI"/>
      <xs:attributeGroup ref="mmd-2.0:def_work-attribute_extension"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="iswc" type="mmd-2.0:def_iswc"/>
  <xs:element name="disc">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="mmd-2.0:sectors"/>
        <xs:element minOccurs="0" ref="mmd-2.0:release-list"/>
        <xs:group ref="mmd-2.0:def_disc-element_extension"/>
      </xs:sequence>
      <xs:attribute name="id" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="[a-zA-Z0-9._]{27}-"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attributeGroup ref="mmd-2.0:def_disc-attribute_extension"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="sectors" type="xs:nonNegativeInteger"/>
  <xs:element name="puid">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="mmd-2.0:recording-list"/>
        <xs:group ref="mmd-2.0:def_puid-element_extension"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:anyURI"/>
      <xs:attributeGroup ref="mmd-2.0:def_puid-attribute_extension"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="isrc">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="mmd-2.0:recording-list"/>
        <xs:group ref="mmd-2.0:def_isrc-element_extension"/>
      </xs:sequence>
      <xs:attribute name="id" type="mmd-2.0:def_isrc"/>
      <xs:attributeGroup ref="mmd-2.0:def_isrc-attribute_extension"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="artist-credit">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="mmd-2.0:name-credit"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="name-credit">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="mmd-2.0:name"/>
        <xs:element ref="mmd-2.0:artist"/>
      </xs:sequence>
      <xs:attribute name="joinphrase"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="relation">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="mmd-2.0:target"/>
        <xs:element minOccurs="0" ref="mmd-2.0:direction"/>
        <xs:element minOccurs="0" ref="mmd-2.0:attributes"/>
        <xs:element minOccurs="0" ref="mmd-2.0:begin"/>
        <xs:element minOccurs="0" ref="mmd-2.0:end"/>
        <xs:choice minOccurs="0">
          <xs:element ref="mmd-2.0:artist"/>
          <xs:element ref="mmd-2.0:release"/>
          <xs:element ref="mmd-2.0:release-group"/>
          <xs:element ref="mmd-2.0:recording"/>
          <xs:element ref="mmd-2.0:label"/>
          <xs:element ref="mmd-2.0:work"/>
          <xs:group ref="mmd-2.0:def_relation-element_extension"/>
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="type" use="required" type="xs:anyURI"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="target" type="xs:anyURI"/>
  <xs:element name="direction" type="mmd-2.0:def_direction"/>
  <xs:element name="attributes" type="mmd-2.0:def_URI-list"/>
  <xs:element name="alias">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element minOccurs="0" ref="mmd-2.0:script"/>
      </xs:sequence>
      <xs:attribute name="type" type="xs:anyURI"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="tag">
    <xs:complexType mixed="true">
      <xs:attribute name="count" type="xs:nonNegativeInteger"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="user-tag" type="xs:string"/>
  <xs:element name="rating">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:float">
          <xs:attribute name="votes-count" type="xs:nonNegativeInteger"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="user-rating" type="xs:nonNegativeInteger"/>
  <xs:element name="label-info">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="mmd-2.0:catalog-number"/>
        <xs:element minOccurs="0" ref="mmd-2.0:label"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="catalog-number" type="xs:string"/>
  <xs:element name="medium">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="mmd-2.0:title"/>
        <xs:element minOccurs="0" ref="mmd-2.0:position"/>
        <xs:element minOccurs="0" ref="mmd-2.0:format"/>
        <xs:element minOccurs="0" ref="mmd-2.0:disc-list"/>
        <xs:group ref="mmd-2.0:def_track-list"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="position" type="xs:nonNegativeInteger"/>
  <xs:element name="format" type="xs:string"/>
  <xs:group name="def_track">
    <xs:sequence>
      <xs:element name="track">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" ref="mmd-2.0:position"/>
            <xs:element minOccurs="0" ref="mmd-2.0:title"/>
            <xs:element ref="mmd-2.0:recording"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:element name="annotation">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="mmd-2.0:entity"/>
        <xs:element ref="mmd-2.0:name"/>
        <xs:element ref="mmd-2.0:text"/>
        <xs:group ref="mmd-2.0:def_annotation-element_extension"/>
      </xs:sequence>
      <xs:attribute name="type"/>
      <xs:attributeGroup ref="mmd-2.0:def_annotation-attribute_extension"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="entity" type="xs:anyURI"/>
  <xs:element name="text" type="xs:string"/>
  <xs:element name="cdstub">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="mmd-2.0:title"/>
        <xs:element minOccurs="0" name="artist" type="xs:string"/>
        <xs:element minOccurs="0" ref="mmd-2.0:barcode"/>
        <xs:element minOccurs="0" ref="mmd-2.0:comment"/>
        <xs:group ref="mmd-2.0:def_nonmb-track-list"/>
        <xs:group ref="mmd-2.0:def_cdstub-element_extension"/>
      </xs:sequence>
      <xs:attribute name="id" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="[a-zA-Z0-9._]{27}-"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attributeGroup ref="mmd-2.0:def_cdstub-attribute_extension"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="freedb-disc">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="mmd-2.0:title"/>
        <xs:element minOccurs="0" name="artist" type="xs:string"/>
        <xs:element minOccurs="0" ref="mmd-2.0:category"/>
        <xs:element minOccurs="0" ref="mmd-2.0:year"/>
        <xs:group ref="mmd-2.0:def_nonmb-track-list"/>
        <xs:group ref="mmd-2.0:def_freedb-disc-element_extension"/>
      </xs:sequence>
      <xs:attribute name="id">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="[a-zA-Z0-9]{8}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attributeGroup ref="mmd-2.0:def_freedb-disc-attribute_extension"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="category" type="xs:string"/>
  <xs:element name="year">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:pattern value="[0-9]{4}"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:group name="def_nonmb-track">
    <xs:sequence>
      <xs:element name="track">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="mmd-2.0:title"/>
            <xs:element minOccurs="0" name="artist" type="xs:string"/>
            <xs:element ref="mmd-2.0:length"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <!-- This part defines the extensions elements and attributes. -->
  <xs:group name="def_metadata-element_extension">
    <xs:sequence>
      <xs:group minOccurs="0" ref="mmd-2.0:def_extension_element"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="def_artist-element_extension">
    <xs:sequence>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="mmd-2.0:def_extension_element"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="def_release-element_extension">
    <xs:sequence>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="mmd-2.0:def_extension_element"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="def_release-group-element_extension">
    <xs:sequence>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="mmd-2.0:def_extension_element"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="def_recording-element_extension">
    <xs:sequence>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="mmd-2.0:def_extension_element"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="def_label-element_extension">
    <xs:sequence>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="mmd-2.0:def_extension_element"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="def_work-element_extension">
    <xs:sequence>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="mmd-2.0:def_extension_element"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="def_puid-element_extension">
    <xs:sequence>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="mmd-2.0:def_extension_element"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="def_isrc-element_extension">
    <xs:sequence>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="mmd-2.0:def_extension_element"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="def_relation-element_extension">
    <xs:sequence>
      <xs:group ref="mmd-2.0:def_extension_element"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="def_annotation-element_extension">
    <xs:sequence>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="mmd-2.0:def_extension_element"/>
    </xs:sequence>
  </xs:group>
